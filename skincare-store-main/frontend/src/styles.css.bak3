  object-fit: cover;
}

.banner-info {
  padding: 1.5rem;
}

.banner-info h3 {
  font-size: 1.2rem;
  color: #2d3748;
  margin-bottom: 0.5rem;
}

.banner-info p {
  color: #6b7280;
