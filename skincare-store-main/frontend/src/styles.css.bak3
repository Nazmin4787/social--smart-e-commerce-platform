/* ==================== CART PAGE STYLES ==================== */

/* Cart Page Container */
.cart-page-container {
  min-height: 100vh;
  background: #f9fafb;
  padding: 2rem 0;
}

.cart-page-wrapper {
  max-width: 1400px;
  margin: 0 auto;
  padding: 0 1.5rem;
}

/* Cart Header */
.cart-header {
  display: flex;
  align-items: center;
  gap: 1rem;
  margin-bottom: 2rem;
  padding-bottom: 1rem;
  border-bottom: 2px solid #e5e7eb;
}

.cart-back-btn {
  width: 40px;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #ffffff;
  border: 2px solid #e5e7eb;
  border-radius: 50%;
  cursor: pointer;
  transition: all 0.3s ease;
  font-size: 1rem;
  color: #374151;
}

.cart-back-btn:hover {
  background: #A8C256;
  border-color: #A8C256;
  color: #ffffff;
  transform: translateX(-3px);
}

.cart-title {
  font-size: 2rem;
  font-weight: 700;
  color: #1f2937;
  margin: 0;
}

.cart-item-count {
  background: #A8C256;
  color: #ffffff;
  padding: 0.375rem 0.875rem;
  border-radius: 20px;
  font-size: 0.875rem;
  font-weight: 600;
}

/* Cart Content Grid */
.cart-content-grid {
  display: grid;
  grid-template-columns: 1fr 400px;
  gap: 2rem;
  align-items: start;
}

/* Cart Items Section */
.cart-items-section {
  display: flex;
  flex-direction: column;
  gap: 1.25rem;
}

.cart-item-card {
  background: #ffffff;
  border-radius: 16px;
  padding: 1.5rem;
  display: grid;
  grid-template-columns: 120px 1fr auto;
  gap: 1.5rem;
  align-items: start;
  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.06);
  transition: all 0.3s ease;
  border: 2px solid transparent;
  position: relative;
}

.cart-item-card:hover {
  box-shadow: 0 4px 16px rgba(0, 0, 0, 0.1);
  border-color: #F0F4E8;
  transform: translateY(-2px);
}

.cart-item-image-wrapper {
  width: 120px;
  height: 120px;
  border-radius: 12px;
  overflow: hidden;
  background: #f9fafb;
  border: 1px solid #e5e7eb;
}

.cart-item-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.cart-item-no-image {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #f3f4f6;
  color: #9ca3af;
  font-size: 2rem;
}

.cart-item-details {
  display: flex;
  flex-direction: column;
  gap: 0.75rem;
  flex: 1;
}

.cart-item-title {
  font-size: 1.125rem;
  font-weight: 600;
  color: #1f2937;
  margin: 0;
  line-height: 1.4;
}

.cart-item-price {
  font-size: 1rem;
  color: #6b7280;
  margin: 0;
}

.cart-item-actions {
  display: flex;
  align-items: center;
  justify-content: space-between;
  gap: 1rem;
  margin-top: 0.5rem;
}

.cart-quantity-control {
  display: flex;
  align-items: center;
  gap: 0;
  background: #f9fafb;
  border: 2px solid #e5e7eb;
  border-radius: 10px;
  overflow: hidden;
}

.quantity-btn {
  width: 36px;
  height: 36px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #ffffff;
  border: none;
  cursor: pointer;
  transition: all 0.3s ease;
  color: #6b7280;
  font-size: 0.875rem;
}

.quantity-btn:hover {
  background: #A8C256;
  color: #ffffff;
}

.quantity-value {
  min-width: 40px;
  text-align: center;
  font-weight: 600;
  color: #1f2937;
  font-size: 1rem;
  padding: 0 0.5rem;
}

.cart-item-total {
  font-size: 1.25rem;
  font-weight: 700;
  color: #1f2937;
}

.cart-item-remove {
  position: absolute;
  top: 1rem;
  right: 1rem;
  width: 36px;
  height: 36px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fee2e2;
  border: none;
  border-radius: 8px;
  cursor: pointer;
  transition: all 0.3s ease;
  color: #dc2626;
  font-size: 0.875rem;
}

.cart-item-remove:hover {
  background: #dc2626;
  color: #ffffff;
  transform: scale(1.1);
}

/* Cart Summary Section */
.cart-summary-section {
  position: sticky;
  top: 2rem;
}

.cart-summary-card {
  background: #ffffff;
  border-radius: 16px;
  padding: 2rem;
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
  border: 1px solid #f0f0f0;
}

.cart-summary-title {
  font-size: 1.5rem;
  font-weight: 700;
  color: #1f2937;
  margin: 0 0 1.5rem 0;
}

.cart-summary-rows {
  display: flex;
  flex-direction: column;
  gap: 1rem;
}

.cart-summary-row {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0.75rem 0;
}

.summary-label {
  font-size: 0.95rem;
  color: #6b7280;
  display: flex;
  align-items: center;
  gap: 0.5rem;
}

.summary-label i {
  margin-right: 0.5rem;
  color: #A8C256;
}

.summary-value {
  font-size: 1rem;
  font-weight: 600;
  color: #1f2937;
}

.summary-free {
  color: #10b981;
  font-weight: 700;
}

.cart-summary-divider {
  height: 2px;
  background: linear-gradient(90deg, transparent, #e5e7eb, transparent);
  margin: 1.5rem 0;
}

.cart-summary-total {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 1.25rem 0;
  margin-bottom: 1.5rem;
}

.total-label {
  font-size: 1.25rem;
  font-weight: 600;
  color: #374151;
}

.total-value {
  font-size: 1.75rem;
  font-weight: 800;
  color: #A8C256;
}

.cart-checkout-btn {
  width: 100%;
  padding: 1rem 2rem;
  background: linear-gradient(135deg, #A8C256 0%, #7FA650 100%);
  color: #ffffff;
  border: none;
  border-radius: 12px;
  font-size: 1.125rem;
  font-weight: 700;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 0.75rem;
  transition: all 0.3s ease;
  box-shadow: 0 4px 12px rgba(236, 72, 153, 0.3);
}

.cart-checkout-btn:hover {
  background: linear-gradient(135deg, #7FA650 0%, #6B8E3F 100%);
  transform: translateY(-2px);
  box-shadow: 0 6px 20px rgba(236, 72, 153, 0.4);
}

.cart-checkout-btn i {
  transition: transform 0.3s ease;
}

.cart-checkout-btn:hover i {
  transform: translateX(4px);
}

.cart-checkout-btn:disabled {
  background: linear-gradient(135deg, #d1d5db 0%, #9ca3af 100%);
  cursor: not-allowed;
  transform: none;
  box-shadow: none;
}

.cart-checkout-btn:disabled:hover {
  background: linear-gradient(135deg, #d1d5db 0%, #9ca3af 100%);
  transform: none;
  box-shadow: none;
}

.cart-savings-badge {
  margin-top: 1.5rem;
  padding: 1rem;
  background: linear-gradient(135deg, #dcfce7 0%, #d1fae5 100%);
  border-radius: 10px;
  display: flex;
  align-items: center;
  gap: 0.75rem;
  border: 1px solid #bbf7d0;
}

.cart-savings-badge i {
  color: #10b981;
  font-size: 1.25rem;
}

.cart-savings-badge span {
  color: #065f46;
  font-size: 0.9rem;
  font-weight: 600;
}

/* Empty Cart State */
.cart-empty-container {
  min-height: 70vh;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 2rem;
}

.cart-empty-content {
  text-align: center;
  max-width: 400px;
}

.cart-empty-content i {
  font-size: 6rem;
  color: #d1d5db;
  margin-bottom: 1.5rem;
}

.cart-empty-content h2 {
  font-size: 1.75rem;
  font-weight: 700;
  color: #374151;
  margin: 0 0 1rem 0;
}

.cart-empty-content p {
  font-size: 1rem;
  color: #6b7280;
  margin: 0 0 2rem 0;
}

/* Loading State */
.cart-loading-container {
  min-height: 70vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  gap: 1.5rem;
}

.loading-spinner {
  width: 50px;
  height: 50px;
  border: 4px solid #f3f4f6;
  border-top-color: #A8C256;
  border-radius: 50%;
  animation: spin 0.8s linear infinite;
}

@keyframes spin {
  to { transform: rotate(360deg); }
}

.cart-loading-container p {
  font-size: 1.125rem;
  color: #6b7280;
  font-weight: 500;
}

/* Responsive Design */
@media (max-width: 1024px) {
  .cart-content-grid {
    grid-template-columns: 1fr;
  }

  .cart-summary-section {
    position: relative;
    top: 0;
  }
}

@media (max-width: 768px) {
  .cart-page-wrapper {
    padding: 0 1rem;
  }

  .cart-header {
    margin-bottom: 1.5rem;
  }

  .cart-title {
    font-size: 1.5rem;
  }

  .cart-item-card {
    grid-template-columns: 100px 1fr;
    gap: 1rem;
    padding: 1.25rem;
  }

  .cart-item-image-wrapper {
    width: 100px;
    height: 100px;
  }

  .cart-item-remove {
    position: relative;
    top: auto;
    right: auto;
    grid-column: 1 / -1;
    width: 100%;
    margin-top: 0.75rem;
  }

  .cart-item-actions {
    flex-direction: column;
    align-items: stretch;
    gap: 0.75rem;
  }

  .cart-item-total {
    text-align: center;
  }

  .cart-summary-card {
    padding: 1.5rem;
  }
}

@media (max-width: 480px) {
  .cart-page-container {
    padding: 1rem 0;
  }

  .cart-back-btn {
    width: 36px;
    height: 36px;
    font-size: 0.875rem;
  }

  .cart-title {
    font-size: 1.25rem;
  }

  .cart-item-title {
    font-size: 1rem;
  }

  .cart-summary-title {
    font-size: 1.25rem;
  }

  .total-value {
    font-size: 1.5rem;
  }
}

